{
	"compilerOptions": {
	   "lib": [
		  "es5",
		  "es6"
	   ],
	   "target": "es5",
	   "module": "commonjs",
	   "esModuleInterop": true,
	   "moduleResolution": "node",
	   "outDir": "./dist",
	   "emitDecoratorMetadata": true,
	   "experimentalDecorators": true,
	   "sourceMap": true,
	   "strict": true,
	   "noImplicitAny": true,
	   "downlevelIteration": true
	}
 }
